using System;

public class Program
{
    public Hero HeroOne;
    public Hero HeroTwo;
    public Hero HeroThree;

    public void Main()
    {
        HeroOne = new Hero();
        HeroTwo = new Hero();
        HeroThree = new Hero();

        HeroOne.health = 2;
        HeroOne.powerlevel = 3;
        HeroOne.speed = 1;

        HeroTwo.health = 3;
        HeroTwo.powerlevel = 1;
        HeroTwo.speed = 2;

        HeroThree.health = 1;
        HeroThree.powerlevel = 2;
        HeroThree.speed = 3;
    }
}

public class Hero
{
    public int health;
    public int powerlevel;
    public int speed;
}